<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet id="ENDTB-201604201208" author="Shireesha, Shruthi">
        <createTable tableName="metadata_code_dictionary">
            <column name="concept_id" type="int(11)"/>
            <column name="name" type="varchar(255)"/>
            <column name="code" type="int(11)"/>
        </createTable>
        <addUniqueConstraint
                             columnNames="concept_id, name"
                             constraintName="metadata_code_key"
                             tableName="metadata_code_dictionary"
                             />
    </changeSet>
    <changeSet id="ENDTB-201604201236" author="Shireesha, Shruthi">
        <insert tableName="metadata_code_dictionary">
            <column name="name" value="M"/>
            <column name="code" valueNumeric="1"/>
        </insert>
        <insert tableName="metadata_code_dictionary">
            <column name="name" value="F"/>
            <column name="code" valueNumeric="2"/>
        </insert>
        <insert tableName="metadata_code_dictionary">
            <column name="name" value="O"/>
            <column name="code" valueNumeric="3"/>
        </insert>
    </changeSet>
    <changeSet id="ENDTB-201605101556" author="Mahesh">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="0">
                select count(*) from concept_answer where concept_id = 792 and answer_concept = 1648
            </sqlCheck>
        </preConditions>
        <comment>Adding 'Unable to or did not access' option to 'Followup, Ankle reflexes in left'</comment>
        <sql>
		insert into concept_answer (concept_id,answer_concept,creator,date_created,sort_weight,uuid) 
		values (792,1648,4,now(),6,uuid());
        </sql>
    </changeSet>
    <changeSet id="ENDTB-201605101601" author="Mahesh">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="0">
                select count(*) from concept_answer where concept_id = 793 and answer_concept = 1648
            </sqlCheck>
        </preConditions>
        <comment>Adding 'Unable to or did not access' option to 'Followup, Vibration Perception in Left'</comment>
        <sql>
                insert into concept_answer (concept_id,answer_concept,creator,date_created,sort_weight,uuid) 
                values (793,1648,4,now(),5,uuid());
        </sql>
    </changeSet>
</databaseChangeLog>
